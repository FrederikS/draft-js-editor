{
  "name": "richie",
  "version": "0.0.1",
  "author": "Frederik Steffen <frederik.steffen@gmail.com>",
  "description": "Rich-Text-Editor implemented with draft.js",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "dist/"
    "examples/"
    "LICENSE"
  ],
  "keywords": [
    "draftjs",
    "draft-js",
    "editor",
    "react",
    "richtext"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FrederikS/richie.git"
  },
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --inline --hot",
    "lint": "node_modules/.bin/eslint index.js src",
    "build:dist": "node_modules/.bin/webpack --config webpack.config.build.js",
    "build:dist:min": "node_modules/.bin/webpack --config webpack.config.build.js --output-filename Richie.min.js --optimize-minimize",
    "build:package": "node_modules/.bin/babel src --out-dir lib --ignore app.js,MuiWrappers.js",
    "build": "npm run clean && npm run build:package && npm run build:dist && npm run build:dist:min",
    "prepublish": "npm run build",
    "clean": "rm -rf dist lib"
  },
  "peerDependencies": {
    "material-ui": "^0.15.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "dependencies": {
    "draft-js": "^0.7.0",
    "interact.js": "^1.2.6",
    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "material-ui": "^0.15.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
